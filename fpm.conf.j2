[cdfqdn]
user                                = cdnuser
group                               = cdnuser
listen                              = cdsock
listen.owner                        = nginx
listen.group                        = nginx
php_admin_value[disable_functions]  = exec,passthru,shell_exec,system
php_admin_flag[allow_url_fopen]     = off
pm                                  = dynamic
pm.max_children                     = 150
pm.start_servers                    = 6
pm.min_spare_servers                = 3
pm.max_spare_servers                = 18
pm.max_requests                     = 500
chdir                               = /	